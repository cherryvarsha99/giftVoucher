<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.project.androidtuts.callbacks">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Giftcenter">
        <activity
            android:name="com.project.androidtuts.callbacks.authentication.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.project.androidtuts.callbacks.GiftCardsActivity"/>
        <activity android:name="com.project.androidtuts.callbacks.MainActivity" />
        <activity android:name="com.project.androidtuts.callbacks.MainMenuActivity" />
        <activity android:name="com.project.androidtuts.callbacks.authentication.RegisterActivity" />
        <activity android:name="com.project.androidtuts.callbacks.authentication.ProfileActivity" />
        <activity android:name="com.project.androidtuts.callbacks.authentication.EditProfileActivity" />
        <activity android:name="com.project.androidtuts.callbacks.authentication.ChangePasswordActivity" />
    </application>

</manifest>